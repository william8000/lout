    def sigmaone        { @Sym @Char "sigma1" }
    def Omega           { @Sym @Char "Omega" }
    def Xi              { @Sym @Char "Xi" }
    def Psi             { @Sym @Char "Psi" }
    def Zeta            { @Sym @Char "Zeta" }
